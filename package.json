{
  "name": "autokin",
  "version": "1.0.0",
  "description": "Autokin - Simple REST Automation by Gherkin",
  "main": "index.js",
  "scripts": {
    "lint": "eslint",
    "features": "cucumber-js --format progress-bar",
    "test": "eslint lib/**/*.js test/**/*.js && nyc --reporter=text --reporter=lcov mocha test -c",
    "test-coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "bin": {
    "autokin": "./bin/autokin"
  },
  "keywords": [
    "autokin",
    "gherkin",
    "REST",
    "API",
    "testing",
    "automation"
  ],
  "author": "Aries Beltran <ariesbe@icloud.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/ariesb/autokin.git"
  },
  "bugs": {
    "url": "https://github.com/ariesb/autokin/issues"
  },
  "dependencies": {
    "chai": "^4.2.0",
    "cli-table3": "^0.5.1",
    "commander": "^2.19.0",
    "cucumber": "^5.1.0",
    "deep-diff": "^1.0.2",
    "jsonpath": "^1.0.0",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "mocha": "^5.2.0",
    "eslint": "^5.12.0",
    "eslint-plugin-mocha": "^5.2.1",
    "coveralls": "^3.0.2",
    "mocha-lcov-reporter": "^1.3.0",
    "nock": "^10.0.6",
    "nyc": "^13.1.0"
  },
  "files": [
    "bin/",
    "lib/"
  ]
